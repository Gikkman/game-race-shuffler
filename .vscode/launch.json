{
  "version": "0.2.0",
  "configurations": [
  {
      "name": "Run Client",
      "type": "node",
      "request": "launch",

      "cwd": "${workspaceFolder}/packages/client",
      "runtimeArgs": [
          "-r",
          "ts-node/register"
      ],
      "args": [
          "src/index.ts"
      ],
      "env": {
        "TS_NODE_PROJECT": "tsconfig.json"
      },
      "outputCapture": "std"
  }
  ]
}
